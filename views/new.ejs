<% include partials/header %>
<h1>Create new asset</h1>
<form action="/assets" method="post">
    <input type="text" name="name" placeholder="Asset Name">
    <input type="text" name="type" placeholder="Asset Type">
    <input type="text" name="location" placeholder="Asset Location">
    <input type="text" name="description" placeholder="Asset Description">
    <input type="text" name="image" placeholder="Asset Image">
    <button type="submit">Submit</button>
</form>

<a href="/asset">Return to Assets</a>

